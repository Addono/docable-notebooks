    "express-pino-logger": "^5.0.0",
    "express-session": "^1.17.1",
    "got": "^11.7.0",
    "infra.connectors": "github:ottomatica/infra.connectors",
    "js-yaml": "^3.14.0",
    "jsdom": "^16.4.0",
    "marked": "^2.0.0",
    "open": "^7.2.1",
    "open-editor": "^2.0.1",
    "pino": "^6.6.1",
    "prettier": "^2.1.2",
    "pug": "^3.0.0",
    "simple-git": "^2.20.1",
    "slash": "^3.0.0",
    "socket.io": "^2.3.0",
    "yargs": "^15.4.1"
  }
